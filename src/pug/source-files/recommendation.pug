mixin box-centered(text, icon, color_class)
	div(class=color_class class="card")
		div.card__header.mb-2
			div.card__box-icon
				span.card__icon.global-icon
					img(src=icon alt="elo")
			h3.card__title Lorem ipsum dolor sit amet consectetur.
		div.card__content
			p.paragraph-text= text	

